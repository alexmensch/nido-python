graft wheelhouse

global-exclude __pycache__ *.pyc *.pyo *.swp *.swo

# Include webapp static files
graft nido/static
graft nido/templates

# Exclude all build environment artefacts
prune instance
prune nido-venv
prune *.egg-info
prune .git
prune build
prune dist
prune node-modules
exclude nohup.out
exclude package-lock.json

# Include auxilliary package files
include *.py*
include *.js
include *.sh
include package.json

# Packaging and install files
include LICENSE
include README.md
include install
include requirements.txt
